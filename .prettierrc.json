{
    "plugins": [
        "@trivago/prettier-plugin-sort-imports"
      ],
    "printWidth": 80,
    "tabWidth": 4,
    "trailingComma": "es5",
    "singleQuote": false,
    "semi": true,
    "importOrderSeparation": true,
    "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/app",
    "^@/pages",
    "^@/widgets",
    "^@/features",
    "^@/entities",
    "^@/shared",
    "^[./]"
    ],
    "importOrderSortSpecifiers": true
}
