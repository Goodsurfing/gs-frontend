{
    "name": "webpack-react-typescript",
    "version": "1.0.0",
    "license": "ISC",
    "author": "kuromukade",
    "scripts": {
        "lint": "eslint src/ --cache --color --ext .jsx,.js,.tsx,.ts --ignore-pattern node_modules",
        "start": "webpack serve --env port=3000",
        "start:analyze": "concurrently \"npm run start --env analyze=true\"",
        "build:prod": "webpack --env mode=production",
        "clean": "rm -rf dist",
        "bootstrap": "mkdir dist",
        "build": "npm run clean && npm run bootstrap && webpack --progress --mode production --profile --json > dist/stats.json",
        "test": "jest --watch",
        "test:coverage": "jest --coverage",
        "bundle-report": "npm run build && webpack-bundle-analyzer dist/stats.json",
        "stylelint": "stylelint \"**/*.scss\" --fix",
        "lint:ts": "npx eslint ./src --fix",
        "lint:scss": "npx stylelint ./src --fix",
        "prettier": "prettier --write \"**/*.{ts,tsx,js}\""
    },
    "devDependencies": {
